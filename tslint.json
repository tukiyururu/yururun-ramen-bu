{
    "extends": "tslint:latest",
    "jsRules": {
        "eofline": true,
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "always"
        ]
    },
    "rules": {
        "interface-name": [
            false,
            "always-prefix"
        ],
        "no-angle-bracket-type-assertion": false,
        "no-console": [
            false
        ],
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "trailing-comma": [
            false
        ]
    }
}